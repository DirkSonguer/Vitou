{
  "server": {
    "connectionPort": "8888",
    "activeModules": [
      "session",
      "user",
      "lobby",
      "game",
      "chat"
    ],
    "gameDirectory": "game_modules"
  },
  "lobby": {
    "minParticipants": 2,
    "maxParticipants": 2
  },
  "logging": {
    "logLevel": 0,
    "logTarget": "CONSOLE",
    "logFile": ""
  },
  "successors": [
    { "if": "_system/game/createfromlobby", "then": "game/successors/ongamecreated" },
    { "if": "_system/user/create", "then": "game/successors/onusercreated" }
  ]
}