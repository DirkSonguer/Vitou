// set tank entity and turret
Player.CurrentTankEntity = Lobby.TankEntity;
Player.CurrentWeaponTurretEntity = Lobby.WeaponTurretEntity;

// initial tank position is random
Player.CurrentTankEntity.PositionComponent.x = random(0, Game.map.maxX);
